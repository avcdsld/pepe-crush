end

function _update()
 if game_state == 0 then
  if btnp(4) then
   game_state=1
   music(-1,300)
   music(music_bgm,0,13)
  end
 elseif game_state == 1 then
  update_game()
 elseif game_state == 2 then
  if btnp(0) or btnp(1) or btnp(2) or btnp(3) then
   gameover_screen_slct=(gameover_screen_slct+1)%2
  end
  if btnp(4) or btnp(5) then
   if gameover_screen_slct == 0 then
    game_state=3
   else
    _init()
    game_state=1
    music(-1,300)
    music(music_bgm,0,13)
   end
  end
  if wait_for_gameover_music > 0 then
   wait_for_gameover_music-=1
  elseif should_play_gameover_music then
   should_play_gameover_music=false
   music(music_gameover,30,13)
  end
 else
  if btnp(4) or btnp(5) then
   game_state=2
  end
 end
end

function print_c(s,x,y,c)
 print(s,x-((#s * 4)/2),y,c)
end

function draw_title()
 for i=1,#star_x do
  local color = 6
  local color2 = 13
  if star_speed[i] < 1.0 then
   color = 13
   color2 = 1
  elseif star_speed[i] < 0.75 then
   color = 1
   color2 = 1
  end

  if star_blink_current_wait[i] > 0 then
   star_blink_current_wait[i]-=1
   if star_blink_current_wait[i] < 6 then
    color = 1
    color2 = 1
   end
  else
   star_blink_current_wait[i]=star_blink_wait[i]
  end

  pset(star_x[i], star_y[i], color)
  pset(star_x[i] + 1, star_y[i], color2)
  pset(star_x[i], star_y[i] + 1, color2)
  pset(star_x[i] - 1, star_y[i], color2)
  pset(star_x[i], star_y[i] - 1, color2)
 end

if wait_for_star > 0 then
 wait_for_star-=1
else
 wait_for_star=2

 for i=1,#star_y do
  local inv=-1
  if flr(rnd(2)) == 0 then
   inv=1
  end

  local sy = star_y[i]
  sy += star_speed[i]
  if sy > 128 then
   sy -= 128
   elseif sy < 0 then
   sy += 128
  end
  star_y[i] = sy

  if flr(rnd(10)) == 0 then
   local sx = star_x[i]
   sx += 1 * inv
   if sx > 128 then
    sx -= 128
   elseif sx < 0 then
    sx += 128
   end
   star_x[i] = sx
  end
 end
 end

 local x=64-16
 local y=64+5

 if wait_for_pepe > 0 then
  wait_for_pepe-=1
 else
  wait_for_pepe=150
 end

 local n=12 -- opened eyes
 if wait_for_pepe < 4 then
  n=64 -- half opened eyes
 elseif wait_for_pepe < 8 then
  n=68 -- closed eyes
 elseif wait_for_pepe < 12 then
  n=64 -- half opened eyes
 elseif wait_for_pepe < 16 then
  n=68 -- closed eyes
 elseif wait_for_pepe < 20 then
  n=64 -- half opened eyes
 end
 spr(n,x,y,4,4)

 function zspr(n,w,h,dx,dy,dz)
  sspr(8*(n%16),8*flr(n/16),8*w,8*h,dx,dy,8*w*dz,8*h*dz)
 end

 zspr(128,7,4,8,16,2)
 print_c("ayalan & 8bitacidlab present", 66, 10, 13) 
 print_c("press z to start", 64, 110, 7)
end

function draw_tiles()
 if wait_for_bomb > 0 then
  wait_for_bomb-=1
 else
  wait_for_bomb=6
 end

 cx=64-(tilew*8)/2+offsetx
 cy=64-(tileh*8)/2+offsety

 for y=0,tileh+1 do
  xpos=cx-1*8
  ypos=cy+(y-1)*8
  spr(33,xpos,ypos)
  xpos=cx+tilew*8
  spr(33, xpos, ypos)
 end
 for x=1,tilew do
  xpos=cx+(x-1)*8
  ypos=cy+tileh*8
  spr(33,xpos,ypos)
 end

 for y=1,tileh do
  for x=1,tilew do
   xpos=cx+(x-1)*8
   ypos=cy+(y-1)*8
   local n=tiles[y][x]
   -- for bomb blink
   if tiles[y][x] > type_num then
    if wait_for_bomb%6 >= 3 then
     n-=bomb_spr_offset
    end
   end
   spr(n,xpos,ypos)
  end
 end

 for y=1,tileh do
  for x=1,tilew do
   xpos=cx+(x-1)*8
   ypos=cy+(y-1)*8

   local c=7
   if cur_slct_x != -1 then
    c=14
   end
   if x == cur_x and y == cur_y then
    rect(xpos-1,ypos-1,xpos+8,ypos+8,c)
   end

   if x==cur_slct_x and y==cur_slct_y and cur_blink > cur_blink_fr/2 then
    rect(xpos,ypos,xpos+7,ypos+7,7)
   end
  end
 end
end

function draw_score()
 if not tiles_initialized then
  return
 end
 print_c("score",106,30,7)
 print_c(""..score,106,40,7)
end

function draw_time_left()
 if not tiles_initialized then
  return
 end
 if time_left > 0 then
  time_left-=1
 end
 if time_left == 20*30 then
  switch_to_hurry_bgm_mode()
 end
 print_c("time",106,55,7)
 print_c(""..flr(time_left/30),106,65,7)
 if display_hint then
  print("hint:"..hint_x..","..hint_y,14,115,6)
 end
end

function draw_pepe()
 local x=92
 local y=80

 if wait_for_angry_pepe > 0 then
  wait_for_angry_pepe-=1
  local n=140 -- angry pepe
  local offset=0
  if wait_for_angry_pepe%4 >= 2 then
   offset=1
  end
  spr(n,x+offset,y,4,4)
  return
 end

 if wait_for_pepe > 0 then
  wait_for_pepe-=1
 else
  wait_for_pepe=150
 end

 local n=12 -- opened eyes
 if wait_for_pepe < 4 then
  n=64 -- half opened eyes
 elseif wait_for_pepe < 8 then
  n=68 -- closed eyes
 elseif wait_for_pepe < 12 then
  n=64 -- half opened eyes
 elseif wait_for_pepe < 16 then
  n=68 -- closed eyes
 elseif wait_for_pepe < 20 then
  n=64 -- half opened eyes
 end
 spr(n,x,y,4,4)
end

function draw_game()
 draw_tiles()
 draw_score()
 draw_time_left()
 draw_pepe()
 print("â¬…ï¸â¬†ï¸â¬‡ï¸âž¡ï¸ move z:select/swap", 13, 120, 13)
end

function _draw()
 cls(0)
 if game_state == 0 then
  draw_title()
 elseif game_state == 1 then
  draw_game()
 elseif game_state == 2 then
  draw_gameover()
 else
  draw_gameover_share()
 end
end

