-- hairpepe super shine
-- by 8bit-acid-lab feat. ayalan

-- tile stuff
tiles={}
type_num=7
tilew=7
tileh=9
offsety=4
offsetx=-15

-- cursor stuff
cur_x=1
cur_y=1
cur_slct_x=-1
cur_slct_y=-1
cur_blink=0
cur_blink_fr=20

-- game stuff
match_cnt=3
game_state=0 -- 0:title, 1:game, 2:gameover
gameover_reason=""
gameover_screen_slct=0

music_start=9
music_bgm=25
music_bgm_fast=33
music_gameover=0
music(music_start,0,13)

function _init()
 for y=1,tileh do
  tiles[y]={}
  for x=1,tilew do
   tiles[y][x]=-1
  end
 end

 tiles_initialized=false
 wait_for_clearing=0
 score=0
 time_left=60*30 --1min
 wait_for_pepe=0
 wait_for_angry_pepe=0

 -- bomb
 wanted_new_bomb_tile_type=-1
 bomb_spr_offset=16
 wait_for_bomb=6

 -- for checking illegal moves
 should_match=false
 swapped_x1=-1
 swapped_y1=-1
 swapped_x2=-1
 swapped_y2=-1

 -- hint message
 display_hint=false
 hint_x=-1
 hint_y=-1

 -- stars in start screen
 star_x={}
 star_y={}
 star_speed={}
 star_blink_wait={}
 star_blink_current_wait={}
 for i=1,30 do
  add(star_x,flr(rnd(128)))
  add(star_y,flr(rnd(128)))
  add(star_speed,rnd(1.0)+0.5)
  local wait=flr(rnd(5*30)+1*30)
  add(star_blink_wait,wait)
  add(star_blink_current_wait,wait)
 end
 wait_for_star=2

 -- for gameover music
 wait_for_gameover_music=0
 should_play_gameover_music=false
end

function inform_invalid_move()
 sfx(23) -- Invalid move
 wait_for_angry_pepe=30
 wait_for_pepe=0
end

function switch_to_normal_bgm_mode()
 music(-1,300)
 music(music_bgm,0,13)
 wait_for_angry_pepe=0
end

function switch_to_hurry_bgm_mode()
 music(-1,300)
 music(music_bgm_fast,0,13)
 wait_for_angry_pepe=20*30 --20sec
end

function move_cursor()
 if btnp(0) then
  if cur_x > 1 then
   cur_x-=1
  else
   inform_invalid_move()
  end
 end
 if btnp(1) then
  if cur_x < tilew then
   cur_x+=1
  else
   inform_invalid_move()
  end
 end
 if btnp(2) then
  if cur_y > 1 then
   cur_y-=1
  else
   inform_invalid_move()
  end
 end
 if btnp(3) then
  if cur_y < tileh then
   cur_y+=1
  else
   inform_invalid_move()
  end
 end
end

function move_cursor_selected()
 -- to revert position if we move too far away from selected tile
 local tmp_x=cur_x
 local tmp_y=cur_y
 move_cursor()
 if abs(cur_slct_x - cur_x) + abs(cur_slct_y - cur_y) > 1 then
  cur_x=tmp_x
  cur_y=tmp_y
  inform_invalid_move()
 end
end

function select_cursor()
 if btnp(4) then
  if cur_slct_x == -1 then
   -- select this tile
   cur_slct_x=cur_x
   cur_slct_y=cur_y
   sfx(21) -- Object select
  else
   -- something was selected
   swap_tiles(cur_slct_x,cur_slct_y,cur_x,cur_y)

   should_match=true
   swapped_x1=cur_slct_x
   swapped_y1=cur_slct_y
   swapped_x2=cur_x
   swapped_y2=cur_y

   if cur_slct_x == cur_x and cur_slct_y == cur_y then
    should_match=false
   else
    sfx(14) -- Swap Position
   end

   -- reset selection
   cur_slct_x=-1
   cur_slct_y=-1
  end
 end
end

function draw_gameover()
 cls(0)
 print("game over",20,25,7)
 print(gameover_reason,20,35,7)
 print("score: "..score,20,50,7)

 if gameover_screen_slct == 0 then
  print("> share",20,80,14)
  print("  play again",20,90,7)
 else
  print("  share",20,80,7)
  print("> play again",20,90,14)
 end

 if wait_for_pepe > 0 then
  wait_for_pepe-=1
 else
  wait_for_pepe=150
 end

 local x=85
 local y=55
 local n=8 -- opened eyes
 if wait_for_pepe < 4 then
  n=64 -- half opened eyes
 elseif wait_for_pepe < 8 then
  n=68 -- closed eyes
 elseif wait_for_pepe < 12 then
  n=64 -- half opened eyes
 elseif wait_for_pepe < 16 then
  n=68 -- closed eyes
 elseif wait_for_pepe < 20 then
  n=64 -- half opened eyes
 end
 spr(n,x,y,4,4)
end

function draw_gameover_share()
 cls(0)

 local x=64-20
 local y=20
 spr(192,x,y,5,4) -- QR code

 local x=64-20
 local y=20+32
 spr(197,x,y,5,1) -- QR code

 print("share on twitter",30,65,7)
 print("don't forget to include a",15,80,7)
 print("screenshot of your score!",15,90,7)

 print("ðŸ…¾ï¸ return to score",26,105,14)
end

function make_bomb(tile_type)
 local cnt=0
 local candidates_x={}
 local candidates_y={}
 for y=1,tileh do
  for x=1,tilew do
   if tiles[y][x] == tile_type then
    cnt+=1
    candidates_x[cnt]=x
    candidates_y[cnt]=y
   end
  end
 end
 if cnt == 0 then
  wanted_new_bomb_tile_type=tile_type+bomb_spr_offset
 else
  local num = flr(rnd(cnt)) + 1
  tiles[candidates_y[num]][candidates_x[num]] = tile_type + bomb_spr_offset
 end
end

function destroy_by_bomb(tile_type)
 local cnt=0
 for y=1,tileh do
  for x=1,tilew do
   if get_tile_type(tiles[y][x]) == tile_type then
    tiles[y][x]=-1
    cnt+=1
   end
  end
 end
 if cnt > 0 then
  sfx(37) -- Bomb
  score+=cnt*cnt
 end
end

function exists_match(x, y)
 local tile_type = get_tile_type(tiles[y][x])

 local cnt=0
 for i=1,tileh do
  if get_tile_type(tiles[i][x]) == tile_type then
   cnt+=1
  elseif match_cnt > cnt and cnt > 0 then
   cnt=0
  end
 end
 if cnt >= match_cnt then
  hint_x=x
  hint_y=y
  return true
 end

 cnt=0
 for i=1,tilew do
  if get_tile_type(tiles[y][i]) == tile_type then
   cnt+=1
  elseif match_cnt > cnt and cnt > 0 then
   cnt=0
  end
 end
 if cnt >= match_cnt then
  hint_x=x
  hint_y=y
  return true
 end

 return false
end

function exists_match_after_move(x0,y0,x1,y1)
 local exists=false
 swap_tiles(x0,y0,x1,y1)
 if exists_match(x0,y0) then
  exists = true
 elseif exists_match(x1,y1) then
  exists = true
 end
 swap_tiles(x0,y0,x1,y1)
 return exists
end

function can_move()
 for y=1,tileh-1 do
  for x=1,tilew-1 do
   if exists_match_after_move(x,y,x+1,y) then
    return true
   end
   if exists_match_after_move(x,y,x,y+1) then
    return true
   end
  end
 end
 for y=1,tileh-1 do
  if exists_match_after_move(tilew,y,tilew,y+1) then
   return true
  end
 end
 for x=1,tilew-1 do
  if exists_match_after_move(x,tileh,x+1,tileh) then
   return true
  end
 end
 return false
end

function get_tile_type(tile)
 if tile > type_num then
  return tile-bomb_spr_offset
 end
 return tile
end

-- returns: max_matched_cnt, tile_type, include_bomb
function clear_match()
 local max_cnt=0
 local include_bomb=false
 local tile_type=-1
 local horizontal_matched=false
 local vertical_matched=false
 for y=1,tileh do
  for x=1,tilew do
   local tt=get_tile_type(tiles[y][x])
   if tile_type > 0 and tt == -1 then
    tt=tile_type
   end
   if tile_type > 0 and not (tile_type == tt) then
    goto continue
   end

   -- horizontal
   local cnt_h=0
   if not horizontal_matched then
    for x1=x,tilew do
     local t=get_tile_type(tiles[y][x1])
     if t == -1 or t == tt then
      cnt_h+=1
     else
      break
     end
    end
   end

   -- vertical
   local cnt_v=0
   if not vertical_matched then
    for y1=y,tileh do
     local t=get_tile_type(tiles[y1][x])
     if t == -1 or t == tt then
      cnt_v+=1
     else
      break
     end
    end
   end

   if cnt_h >= match_cnt then
    local include_bomb_h=clear_horizontally(x,y,cnt_h)
    include_bomb=include_bomb or include_bomb_h
    score+=cnt_h*cnt_h
    tile_type=tt
    horizontal_matched=true
    if cnt_h > max_cnt then
     max_cnt=cnt_h
    end
   end
   if cnt_v >= match_cnt then
    local include_bomb_v=clear_vertically(x,y,cnt_v)
    include_bomb=include_bomb or include_bomb_v
    score+=cnt_v*cnt_v
    tile_type=tt
    vertical_matched=true
    if cnt_v > max_cnt then
     max_cnt=cnt_v
    end
   end

   ::continue::
  end
 end
 if horizontal_matched or vertical_matched then
  return max_cnt,tile_type,include_bomb
 end
 return 0,0,false
end

function clear_horizontally(x,y,cnt)
 local include_bomb=false
 for i=0,cnt-1 do
  if tiles[y][x+i] > type_num then
   include_bomb=true
  end
  tiles[y][x+i]=-1
 end
 return include_bomb
end

function clear_vertically(x,y,cnt)
 local include_bomb=false
 for i=0,cnt-1 do
  if y+i > tileh then
   break
  end
  if tiles[y+i][x] > type_num then
   include_bomb=true
  end
  tiles[y+i][x]=-1
 end
 return include_bomb
end

function exists_empty_tiles()
 for y=1,tileh do
  for x=1,tilew do
   if tiles[y][x] < 1 then
    return true
   end
  end
 end
 return false
end

function swap_tiles(x0,y0,x1,y1)
 t0=tiles[y0][x0]
 t1=tiles[y1][x1]
 tiles[y0][x0]=t1
 tiles[y1][x1]=t0
end

function move_down_tiles()
 for y=tileh,2,-1 do
  for x=1,tilew do
   if tiles[y][x] < 1 and tiles[y-1][x] > 0 then
    swap_tiles(x,y,x,y-1)
    if tiles_initialized then
     return
    end
   end
  end
 end
end

function fill_top_tiles()
 for x=1,tilew do
  if tiles[1][x] < 1 then
   if wanted_new_bomb_tile_type > 0 then
    tiles[1][x]=wanted_new_bomb_tile_type
    wanted_new_bomb_tile_type=-1
   else
    tiles[1][x]=flr(rnd(type_num))+1
   end
  end
 end
end

function update_game()
 if not tiles_initialized then
  score=0
  time_left=5*30 --1min
 end

 if exists_empty_tiles() then
  move_down_tiles()
  fill_top_tiles()
  return
 end

 if wait_for_clearing > 0 then
  wait_for_clearing-=1
  return
 end

 local matched_cnt,matched_tile_type,include_bomb=clear_match()
 if matched_cnt > 0 then
  should_match=false
  sfx(20) -- Line Clear
  if (time_left < 20*30) and (time_left > 15*30) then
   switch_to_normal_bgm_mode()
  end
  time_left+=5*30
  wait_for_clearing=5
  if include_bomb then
   destroy_by_bomb(matched_tile_type)
  end
  if matched_cnt >= 4 then
   make_bomb(matched_tile_type)
  end
  return
 elseif should_match then
  should_match=false
  swap_tiles(swapped_x1,swapped_y1,swapped_x2,swapped_y2)
  inform_invalid_move()
 end

 if not tiles_initialized then
  tiles_initialized = true
 end

 if (not can_move() or time_left <= 0) and game_state != 2 then
  game_state=2
  if time_left <= 0 then
   gameover_reason="out of time!"
  else
   gameover_reason="no remaining moves!"
  end
  gameover_screen_slct=0
  music(-1,300)
  sfx(22) -- Gameover
  wait_for_gameover_music=3.5*30 --3.5sec
  should_play_gameover_music=true
 end

 cur_blink+=1
 if cur_blink > cur_blink_fr then
  cur_blink=0
 end

 -- cursor movement
 if cur_slct_x == -1 then
  move_cursor()
 else
  move_cursor_selected()
 end
 select_cursor()

-- press 'x' + 'â¬‡ï¸' to display hint
